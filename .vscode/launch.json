    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/ambulance-api-service/main.go",
            "env": {
                "AMBULANCE_API_PORT": "8080",
                "AMBULANCE_API_MONGODB_HOST": "localhost",
                "AMBULANCE_API_MONGODB_PORT": "27017",
                "AMBULANCE_API_MONGODB_DATABASE": "cv1-ambulance",
                "AMBULANCE_API_MONGODB_COLLECTION": "ambulance",
                "AMBULANCE_API_MONGODB_USERNAME": "admin",
                "AMBULANCE_API_MONGODB_PASSWORD": "admin",
                "AMBULANCE_API_MONGODB_TIMEOUT_SECONDS": "5"
            },
        }
    ]
}